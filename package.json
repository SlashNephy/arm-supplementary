{
  "name": "arm-supplementary",
  "version": "1.0.0",
  "author": "SlashNephy",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/arm-supplementary"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "ts-node bin/generate.ts",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check ."
  },
  "dependencies": {
    "@kawaiioverflow/arm": "0.102.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "18.11.4",
    "concurrently": "7.5.0",
    "eslint": "8.26.0",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
